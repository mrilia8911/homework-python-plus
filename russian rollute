import turtle
import random
from tkinter import *
from tkinter import messagebox

tut = turtle.Screen()

tut.bgcolor("white")

tut.title("russian rollute")


myPen = turtle.Turtle()


myPen.up()
myPen.setpos(-150,50)
myPen.down()


myPen.color("yellow")
myPen.fillcolor("yellow")
myPen.begin_fill()

tut = turtle.Screen()



for i in range(4) :
    myPen.forward(50)
    myPen.left(90)


myPen.end_fill()

"""
 /\
 ||
first cube

going to second cube

"""


myPen.up()
myPen.setpos(-50,50)
myPen.down()


myPen.color("yellow")
myPen.fillcolor("yellow")
myPen.begin_fill()

tut = turtle.Screen()



for i in range(4) :
    myPen.forward(50)
    myPen.left(90)


myPen.end_fill()


"""
going to third cube

"""
myPen.up()
myPen.setpos(50,50)
