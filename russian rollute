messagebox.showinfo("tips", "hello! welcome to russian rollute , you have to pick one cube and if its green , it means your safe , and if its red , it means you lost")
name = tut.textinput("name","enter your name :")

r = random.randint(1,6)
if name == "god" :
    print(r)

pick = int(tut.textinput("pick","pick one of the cubes from 1 to 6"))
if pick > 6 or pick < 1 :
    print("wrong play again")
    exit()

if r == 1 :
    if pick == 2 :
        myPen.up()
        myPen.setpos(-50, 50)
        myPen.down()

        myPen.color("green")
        myPen.fillcolor("green")
        myPen.begin_fill()

        tut = turtle.Screen()

        for i in range(4):
            myPen.forward(50)
            myPen.left(90)

        myPen.end_fill()
        messagebox.showinfo("S","your safe")

    elif pick == 3:

        myPen.up()
        myPen.setpos(50, 50)
        myPen.down()

