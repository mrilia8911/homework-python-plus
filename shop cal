print("ranking total number of sold 3 days :")

add1 = item1d1 + item2d1 + item3d1
add2 = item1d2 + item2d2 + item3d2
add3 = item1d3 + item2d3 + item3d3

if add1 == add2 == add3 :
    print("all 3 days sold equal items")
elif add1 == add2 > add3 :
    print("day 1 and 2 sold more items")
elif add1 == add2 < add3 :
    print("day 3 sold more items")
elif add1 < add2 == add3 :
    print("day 2 and 3 sold more items")
elif add1 > add2 == add3 :
    print("day 1 sold more items")
elif add1 > add2 > add3 :
    print("day 1 sold more items")
elif add1 < add2 < add3 :
    print("day 3 sold more items")
elif add1 < add2 > add3 :
    print("day 2 sold more items")
elif add1 > add2 < add3 :
    if add1 == add3 :
        print("day 1 and 3 sold more items")
    elif add1 > add3 :
        print("day 1 sold more items")
    elif add1 < add3 :
        print("day 3 sold more items")
    else :
        print("error , try again")
else :
    print("error , try again")
